{
    "name": "rawphp/raw-router",
    "type": "library",
    "description": "RawRouter is a simple router used by RawPHP framework and other applications.",
    "keywords": ["routing"],
    "homepage": "http://rawphp.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Kaczocha",
            "email": "tom@rawphp.org"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "rawphp/raw-support": "dev-master",
        "rawphp/raw-dispatcher": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.5",
        "rawphp/raw-code-standards": "1.*@dev",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "RawPHP\\RawRouter\\": "src/RawPHP/RawRouter/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "suggest": {
        "rawphp/raw-request": "RawRouter makes it easier in creating urls and routes for the router."
    }
}
